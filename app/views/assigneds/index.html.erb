<h1>Listing assigneds</h1>

<table>
  <tr>
    <th>Student</th>
    <th>Course</th>
    <th>Assignment</th>
    <th>Score</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @assigneds.each do |assigned| %>
  <tr>
    <td><%= assigned.student_id %></td>
    <td><%= assigned.course_id %></td>
    <td><%= assigned.assignment_id %></td>
    <td><%= assigned.score %></td>
    <td><%= link_to 'Show', assigned %></td>
    <td><%= link_to 'Edit', edit_assigned_path(assigned) %></td>
    <td><%= link_to 'Destroy', assigned, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Assigned', new_assigned_path %>
